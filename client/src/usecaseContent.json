{
    "atom": {
        "label": "Atom Fashion",
        "vignette": {
            "name": "Atom",
            "logo": ""
        },
        "layout": "right-appbar",
        "demoComponents": [
            {
                "type": "simple dashboard",
                "label": "Sales Overview",
                "description": "Overview of all your sales!",
                "icon": "fa-chart-line",
                "lookerContent": [
                    {
                        "type": "dashboard",
                        "lookerMethod": "embedDashboard",
                        "id": "131",
                        "label": "Sales Overview",
                        "isNext": false
                    }
                ]
            },
            {
                "type": "custom filter",
                "label": "Inventory Overview",
                "description": "Overview of your inventory",
                "icon": "fa-tshirt",
                "lookerContent": [
                    {
                        "type": "dashboard",
                        "lookerMethod": "embedDashboard",
                        "id": "139",
                        "isNext": false,
                        "label": "Inventory Overview",
                        "filter": {
                            "label": "Select Product Name",
                            "filterName": "Product Name",
                            "lookerMethod": "runQuery",
                            "queryId": "28777",
                            "resultFormat": "json"
                        }
                    }
                ]
            },
            {
                "type": "custom vis",
                "label": "Custom vis",
                "description": "Ayeeeeeee",
                "icon": "fa-dog",
                "lookerContent": [
                    {
                        "type": "custom vis",
                        "lookerMethod": "runInlineQuery",
                        "label": "Custom Viz",
                        "inlineQuery": {
                            "model": "atom_fashion",
                            "view": "order_items",
                            "fields": [
                                "order_items.created_date",
                                "products.category",
                                "order_items.order_count",
                                "order_items.total_sale_price"
                            ],
                            "query_timezone": "America/Los_Angeles"
                        },
                        "desiredFilterName": "products.brand"
                    }
                ]
            }
        ]
    }
}