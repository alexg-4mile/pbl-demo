                    {
                        "type": "explorelite",
                        "id": "explorelite",
                        "label": "Build & Run Queries",
                        "queryBody": {
                            "model": "thelook_adwords",
                            "view": "events",
                            "fields": [
                                "campaigns.campaign_id",
                                "campaigns.campaign_name",
                                "campaigns.campaign_type",
                                "session_purchase_facts.ROI",
                                "adevents.cost_per_click",
                                "adevents.total_cost",
                                "adevents.total_clicks"
                            ],
                            "filters": {
                                "campaigns.is_active_now": "Yes",
                                "campaigns.created_date": "1 year"
                            }
                        },
                        "resultFormat": "json",
                        "fieldType": {
                            "campaigns.campaign_id": "dimension",
                            "campaigns.campaign_name": "dimension",
                            "campaigns.campaign_type": "dimension",
                            "session_purchase_facts.ROI": "measure",
                            "adevents.cost_per_click": "measure",
                            "adevents.total_cost": "measure",
                            "adevents.total_clicks": "measure"
                        },
                        "filterType": {
                            "campaigns.is_active_now": "yesno",
                            "campaigns.created_date": "date"
                        }
                    }
                ]
            },
            {
                "type": "cohort builder",
                "label": "Cohort Builder",
                "icon": "fa-hammer",
                "lookerContent": [
                    {
