            },
            {
                "type": "report builder",
                "label": "Saved Reports",
                "description": "See how you can easily embed reports and give users the power to create new ones",
                "icon": "fa-hammer",
                "lookerContent": [
                    {
                        "type": "folder",
                        "id": "1827",
                        "label": "Saved Reports"
                    },
                    {
                        "type": "explore",
                        "id": "thelook_adwords::events",
                        "label": "Create New Report"
                    }
                ]
            },
            {
                "type": "query builder",
                "label": "Query Builder",
                "description": "Build a query",
                "icon": "fa-search",
                "lookerContent": [
                    {
                        "type": "explorelite",
                        "id": "explorelite",
                        "label": "Build & Run Queries",
                        "queryBody": {
                            "model": "thelook_adwords",
                            "view": "events",
                            "fields": [
                                "campaigns.campaign_id",
                                "campaigns.campaign_name",
                                "campaigns.campaign_type",
                                "session_purchase_facts.ROI",
                                "adevents.cost_per_click",
                                "adevents.total_cost",
                                "adevents.total_clicks"
                            ],
                            "filters": {
                                "campaigns.is_active_now": "Yes",
                                "campaigns.created_date": "6 months"
                            }
                        },
                        "resultFormat": "json",
                        "fieldType": {
                            "campaigns.campaign_id": "dimension",
                            "campaigns.campaign_name": "dimension",
                            "campaigns.campaign_type": "dimension",
                            "session_purchase_facts.ROI": "measure",
                            "adevents.cost_per_click": "measure",
                            "adevents.total_cost": "measure",
                            "adevents.total_clicks": "measure"
                        },
                        "filterType": {
                            "campaigns.is_active_now": "yesno",
                            "campaigns.created_date": "date"
                        }
                    }
                ]
            },
